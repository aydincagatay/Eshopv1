{% block title %}New Image{% endblock %}

{% block body %}
    <h1>Add new Image to GALLERY</h1>

    {{ include('admin/image/_form.html.twig') }}


    {% for image in imagelist %}
        <a href="{{ path('admin_image_del', {'id':image.id,'pid':image.productid}) }}"
           class="btn btn-xs btn-primary btn-block">sil</a>
        <img src="{{ asset('uploads') }}/images/{{ image.image }}">
        <br>
    {% endfor %}


{% endblock %}
