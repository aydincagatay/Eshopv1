{% extends 'frontbase.html.twig' %}
{% block title %}Profil Bilgileri{% endblock %}
{% block sidebar %}{% include 'home/_usersidebar.html.twig' %}{% endblock %}
{% block content %}
    <div style="margin-right: 30px;margin-left: 323px">
        <div class="breadcrumbs">
            <ol class="breadcrumb">
                <li><a href="{{ path('home') }}">Home</a></li>
                <li class="active">Profil bilgilerim</li>
            </ol>
        </div><!--/breadcrums-->
        <h3>Üye Profil Bilgileri</h3>
        <table class="table table-condensed">
            <tbody>
            <tr>
                <th>id</th>
                <td>{{ app.user.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ app.user.email }}</td>
            </tr>
            <tr>
                <th>Roller</th>
                <td>{{ app.user.roles|join }}</td>
            </tr>
            <tr>
                <th>İsim</th>
                <td>{{ app.user.name }}</td>
            </tr>
            <tr>
                <th>Adres</th>
                <td>{{ app.user.address }}</td>
            </tr>
            <tr>
                <th>Şehir</th>
                <td>{{ app.user.city }}</td>
            </tr>
            <tr>
                <th>Telefon</th>
                <td>{{ app.user.phone }}</td>
            </tr>

            </tbody>
        </table>
    </div>
{% endblock %}
